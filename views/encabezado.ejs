        </div>
    
 <section class="container">
        <% if(mensajeExito && mensajeExito.length > 0) { %>
        <div class="alert alert-success alert-dismissible fade show" role="alert">
           	&#128077 <strong class = "alertMessage"> <%= mensajeExito %> </strong>
            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times</span></button>
        </div>
        <% } %>
        
        <% if(mensajeError && mensajeError.length > 0) { %>
        <div class="alert alert-danger alert-dismissible fade show" role="alert">
            <strong class = "alertMessage"> <%= mensajeError %> </strong>
            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span  aria-hidden="true">&times</span></button>
        </div>
        <% } %>
        
        
        <% if(mensajeAdvertencia && mensajeAdvertencia.length > 0) { %>
        <div class="alert alert-warning alert-dismissible fade show" role="alert">
            <strong class = "alertMessage"> <%= mensajeAdvertencia %> </strong>
            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times</span></button>
        </div>
        <% } %>
        
    </section>
        